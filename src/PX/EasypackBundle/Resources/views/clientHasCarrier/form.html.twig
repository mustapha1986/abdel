 <div class="well">
     {{ form_start(formPrincipale , {'attr' : {'class' : 'form-horizontal'}}) }}
     {{ form_errors(formPrincipale) }}

     <div class="form-group">
         {{ form_label(formPrincipale.client , "Nom client" , {'label_attr' : {'class' : 'col-sm-3 control-label'}}) }}
         {{ form_errors(formPrincipale.client) }}

         <div class="col-sm-4">
             {{ form_widget(formPrincipale.client , {'attr' : {'class' : 'clientClass form-control'}}) }}
         </div>
     </div>





     <div class="form-group">
         {{ form_label(formPrincipale.code , "Code" , {'label_attr' : {'class' : 'col-sm-3 control-label'}}) }}
         {{ form_errors(formPrincipale.code) }}

         <div class="col-sm-4">
             {{ form_widget(formPrincipale.code , {'attr' : {'class' : 'carrierClass form-control'}}) }}
         </div>
     </div>


     <div class="form-group">
         <select id="types" class="col-sm-3 form-control">
             <option selected="selected"></option>
             {% for type in types|keys %}
                 <option value="transporteur_type_{{ loop.index }}">{{ type }}</option>
             {% endfor %}
         </select>

     </div>






     {% for form in forms %}
         <div class="form formhidden" id="transporteur_type_{{ loop.index }}">
             {{ form_start(form) }}
             {{ form_widget(form) }}
             {{ form_end(form) }}
         </div>
     {% endfor %}



     <div class="form-group">
         <div class="col-sm-3">
         </div>
         <div class="col-sm-4">

             {{ form_widget(formPrincipale.save, {'attr': {'class': 'btn btn-primary'}}) }}
         </div>
     </div>

     {{ form_rest(formPrincipale) }}
     {{ form_end(formPrincipale) }}
 </div>



